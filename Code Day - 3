Macros ----

index=main sourcetype=csv current_ticket_state="$state$" severity=$sev$ | chart count by severity, current_ticket_state

output lookup -----

|makeresults | eval ticket_number="00000", time_taken="100hrs" | fields - _time| outputlookup vk_sample_lookup.csv append=true


join -------------

index="main" sourcetype="csv" 
| table ticket_number, severity, current_ticket_state 
| join type=inner ticket_number 
    [ search index="vk_join_idx" sourcetype="csv" 
    | table ticket_number, time_taken]

Overwrite ---------------

index="main" sourcetype="csv" 
| table ticket_number, severity, current_ticket_state 
| eval time_taken="NULL" 
| join type=left ticket_number overwrite=false
    [ search index="vk_join_idx" sourcetype="csv" 
    | head 10 
    | table ticket_number, time_taken]

Append ---- 

index=main sourcetype=csv | table ticket_number, severity, current_ticket_state | head 5 | append [search index=vk_join_idx | table time_taken | head 5]


AppendCols -----

index=main sourcetype=csv | table ticket_number, severity, current_ticket_state | head 5 | appendcols [search index=vk_join_idx | table time_taken | head 5]
